format = """
[](#2C5D4C)\
$os\
$username\
[](bg:#4F6B4E fg:#2C5D4C)\
$directory\
[](fg:#4F6B4E bg:#5F8B39)\
$git_branch\
$git_status\
[](fg:#5F8B39 bg:#37935D)\
$c\
$elixir\
$elm\
$golang\
$gradle\
$haskell\
$java\
$julia\
$nodejs\
$nim\
$rust\
$scala\
[](fg:#37935D bg:#5D9A50)\
$docker_context\
[](fg:#5D9A50 bg:#9DAD55)\
$time\
[ ](fg:#9DAD55)\
"""

# Disable the blank line at the start of the prompt
# add_newline = false

# You can also replace your username with a neat symbol like   or disable this
# and use the os module below
[username]
show_always = true
style_user = "bg:#2C5D4C"
style_root = "bg:#2C5D4C"
format = '[$user ](fg:#FFFFFF $style)'
disabled = false

# An alternative to the username module which displays a symbol that
# represents the current operating system
[os]
style = "bg:#2C5D4C"
disabled = true # Disabled by default

[directory]
style = "bg:#4F6B4E"
format = "[ $path ](fg:#FFFFFF $style)"
truncation_length = 3
truncation_symbol = "…/"

# Here is how you can shorten some long paths by text replacement
# similar to mapped_locations in Oh My Posh:
[directory.substitutions]
"Documents" = "󰈙 "
"Downloads" = " "
"Music" = " "
"Pictures" = " "
# Keep in mind that the order matters. For example:
# "Important Documents" = " 󰈙 "
# will not be replaced, because "Documents" was already substituted before.
# So either put "Important Documents" before "Documents" or use the substituted version:
# "Important 󰈙 " = " 󰈙 "

[c]
symbol = " "
style = "bg:#37935D"
format = '[ $symbol ($version) ](fg:#FFFFFF $style)'

[docker_context]
symbol = " "
style = "bg:#5D9A50"
format = '[ $symbol $context ](fg:#FFFFFF $style)'

[elixir]
symbol = " "
style = "bg:#37935D"
format = '[ $symbol ($version) ](fg: #FFFFFF$style)'

[elm]
symbol = " "
style = "bg:#37935D"
format = '[ $symbol ($version) ](fg:#FFFFFF $style)'

[git_branch]
symbol = ""
style = "bg:#5F8B39"
format = '[ $symbol $branch ](fg:#FFFFFF $style)'

[git_status]
style = "bg:#5F8B39"
format = '[$all_status$ahead_behind ](fg:#FFFFFF $style)'

[golang]
symbol = " "
style = "bg:#37935D"
format = '[ $symbol ($version) ](fg:#FFFFFF $style)'

[gradle]
style = "bg:#37935D"
format = '[ $symbol ($version) ](fg:#FFFFFF $style)'

[haskell]
symbol = " "
style = "bg:#37935D"
format = '[ $symbol ($version) ](fg:#FFFFFF $style)'

[java]
symbol = " "
style = "bg:#37935D"
format = '[ $symbol ($version) ](fg:#FFFFFF $style)'

[julia]
symbol = " "
style = "bg:#37935D"
format = '[ $symbol ($version) ](fg:#FFFFFF $style)'

[nodejs]
symbol = ""
style = "bg:#37935D"
format = '[ $symbol ($version) ](fg:#FFFFFF $style)'

[nim]
symbol = "󰆥 "
style = "bg:#37935D"
format = '[ $symbol ($version) ](fg:#FFFFFF $style)'

[rust]
symbol = ""
style = "bg:#37935D"
format = '[ $symbol ($version) ](fg:#FFFFFF $style)'

[scala]
symbol = " "
style = "bg:#37935D"
format = '[ $symbol ($version) ](fg:#FFFFFF $style)'

[time]
disabled = false
time_format = "%R" # Hour:Minute Format
style = "bg:#9DAD55"
format = '[ 󰋑 $time ](fg:#FFFFFF $style)'
